{
  "compilerOptions": {
    "outDir": "Scripts/_vsCompile",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,

    "module": "esnext",
    "moduleResolution": "node",

    "target": "es5",
    "jsx": "react",
    "lib": [ "es6", "dom" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
